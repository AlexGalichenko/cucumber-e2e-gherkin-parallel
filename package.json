{
  "name": "@cucumber-e2e/gherkin-parallel",
  "version": "0.1.3",
  "description": "Feature file splitter for cucumber parallelization",
  "main": "index.js",
  "scripts": {
    "docs": "jsdoc ./src/ -r -t ./node_modules/minami",
    "test": "jest",
    "build": "babel src -d lib",
    "prepublish": "npm run build"
  },
  "keywords": [
    "cucumber",
    "gherkin",
    "parallel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexGalichenko/cucumber-e2e-gherkin-parallel.git"
  },
  "bugs": {
    "url": "https://github.com/AlexGalichenko/cucumber-e2e-gherkin-parallel/issues",
    "email": "alexandr.galichenko@gmail.com"
  },
  "author": "Alexander Galichenko",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "6"
          }
        }
      ]
    ]
  },
  "engines" : {
    "node" : ">=6.0.0"
  },
  "jest": {
    "transformIgnorePatterns": ["<rootDir>\/.+\/"]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "fs-extra": "^7.0.1",
    "jest": "^23.6.0",
    "jsdoc": "^3.5.5",
    "minami": "^1.2.3"
  },
  "dependencies": {
    "cucumber-tag-expressions": "^1.1.1",
    "gherkin": "^5.1.0",
    "glob": "^7.1.3",
    "lodash": "^4.17.11"
  }
}
